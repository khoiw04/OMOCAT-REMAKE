{
  "name": "shop-khoi",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port 3000\" \"bun --watch run ./src/api/server.ts\"",
    "server": "bun --watch run ./src/api/server.ts",
    "client": "vite --port 3000",
    "start": "bun run ./src/api/server.ts",
    "build": "vite build",
    "deploy": "bun run build && wrangler deploy",
    "preview": "concurrently \"bun run ./src/api/server.ts\" \"bun run build && vite preview\"",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "stripe": "stripe listen --forward to localhost:3000/webhook"
